{
  "extends": "../shared/tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "target": "ES2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}